global
updating procedure CleanUserActionsMn(record RcVc RepSpec)
begin
  record HTSUserActionLogVc HtsUAr;
  Boolean found;
  
  found = true;
  HtsUAr.EventDate = RepSpec.sStartDate;
  DBHold;
  while (LoopKey("EventDate",HtsUAr,1,found)) begin
    if (DateInRange(HtsUAr.EventDate,RepSpec.sStartDate,RepSpec.sEndDate)==false) then begin
      found = false;
    end;
    if (found) then begin
      RecordDelete(HtsUAr);
      StepBack(HtsUAr)
    end;
  end;
  DBFlush;
  return;
end;
