external updating procedure CreateIntercompanyTransactionsFromTR(record TRVc);

global
updating procedure HWITRMn(record RcVc RepSpec)
begin
  record TRVc TRr;
  Boolean found;
  
  found = true;
  TRr.TransDate = RepSpec.sStartDate;
  while (loopKey("TransDate",TRr,1,found)) begin
    if (TRr.TransDate>RepSpec.sEndDate) then begin found = false; end;
    if (found) then begin
      CreateIntercompanyTransactionsFromTR(TRr);
    end;
  end;
  return;
end;
