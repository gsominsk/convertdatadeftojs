global
updating procedure ASPDelCloudMachineStatMn(record RcVc RepSpec)
begin
  record CloudMachineStatsVc CMSr;
  Boolean found;
  
  found = true;
  CMSr.Date = RepSpec.sStartDate;
  while (LoopKey("Date",CMSr,1,found)) begin
    if (DateInRange(CMSr.Date,RepSpec.sStartDate,RepSpec.sEndDate)==false) then begin
      found = false;
    end;
    if (found) then begin
      RecordDelete(CMSr);
      StepBack(CMSr)
    end;
  end;
  return;
end;