global
updating procedure DelSynchHistMn(record RcVc RepSpec)
begin
  record UUIDHistoryVc UUIDHistoryr;
  Boolean foundf;
  
  SetDatabaseToUnsafeMode;
  foundf = true;
  while (LoopMain(UUIDHistoryr,1,foundf)) begin
    RecordDelete(UUIDHistoryr);
    StepBack(UUIDHistoryr);
  end;
  SetDatabaseToSafeMode;
  return;
end;

