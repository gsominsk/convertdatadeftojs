global
updating procedure DeactivateAutoRepliesMn(record RcVc RepSpec)
begin
  record ConfAutoReplyVc CARr;
  record ConfAutoReplyVc oldCARr;
  Boolean found;
  
  found = true;
  while (LoopMain(CARr,1,found)) begin
    if (found) then begin
      RecordCopy(oldCARr,CARr);
      CARr.Activated = 0;
      RecordUpdate(oldCARr,CARr,false);
    end;
  end;
  return;
end;