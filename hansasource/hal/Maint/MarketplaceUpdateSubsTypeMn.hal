global
updating procedure MarketplaceUpdateSubsTypeMn(record RcVc RepSpec)
begin
  record SubscriptionVc Subr;
  record SubscriptionVc oldSubr;
  record SubItemVc SIr;
  Boolean found;
  
  found = true;
  while (LoopMain(Subr,1,found)) begin
    if (found) then begin
      SIr.SerNr = Subr.SubIN;
      if (ReadFirstMain(SIr,1,true)) then begin
        RecordCopy(oldSubr,Subr);
        Subr.Type = SIr.Type;
        RecordUpdate(oldSubr,Subr,false);
      end;
    end;
  end;
  return;
end;