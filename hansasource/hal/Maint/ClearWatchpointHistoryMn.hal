global
updating procedure ClearWatchpointHistoryMn(record RCVc RepSpec)
begin
  record SWMVc SWMr;

  while (LoopMain(SWMr,1,true)) begin
    if (RepSpec.flags[0]==0 or (RepSpec.flags[0]==1 and SWMr.DurStatus==kWatchpointStatusActive)) then begin
      RecordRemove(SWMr);
      StepBack(SWMr);
    end;
  end;

  return;
end;