global
procedure PrintSalesContract(LongInt ORCONr)
begin
  record ORCOVc ORCOr;
  
  if (ORCONr>0) then begin
    ORCOr.SerNr = ORCONr;
    ReadFirstMain(ORCOr,1,true);
    OUTFORMFIELD("F_ORCONR",ORCOr.SerNr);  
    OUTFORMFIELD("F_ORCODATE",ORCOr.CODate);  
    OUTFORMFIELD("F_ORCOPAYDEAL",ORCOr.PayDeal);  
    OUTFORMFIELD("F_ORCOTYPE",ORCOr.Type);  
  end;  
  return;
end;

procedure ORPrintNote(record ORVc ORr,string argumentstr)
begin
  record NotepadVc Noter;
  record RLinkVc RLr;
  Integer notenr;

  notenr = 1;
  while (ReadRecordLink(ORr,notenr,Noter,RLr)) begin
    if ((argumentstr==RLr.Comment) or (blank(argumentstr))) then begin
      PrintNote(Noter);
      goto LORPrintNote;
    end;
    notenr = notenr + 1;
  end;
LORPrintNote:;  
  return;
end;

