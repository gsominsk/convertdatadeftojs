  if (HasLocalization("PRT")) then begin
    OUTFORMFIELD("F_OFFICIALSERNO",GetOffSerNrIVJournalID(ORr.SerNr,"ORVc",ORr.OfficialSerNr) & " " & ORr.OfficialSerNrSerie & "/" & Right(ORr.OfficialSerNr,len(ORr.OfficialSerNr)-len(ORr.OfficialSerNrSerie)));
  end else begin
