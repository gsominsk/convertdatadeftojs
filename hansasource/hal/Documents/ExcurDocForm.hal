global
procedure ExcurDocForm(record RcVc RepSpec)
BEGIN
  record ExcursionVc Excurr;
  Boolean TrHs,skip;
  LongInt afr,ato;
  Boolean tmpf,previewf;
  
  StartPrintDialogGroup;
  previewf = false;
  afr = FirstInRange(RepSpec.f1,10);
  ato = LastInRange(RepSpec.f1,10);
  Excurr.SerNr = afr;
  TrHs = true;
  while (LoopMain(Excurr,1,TrHs)) begin
  
    skip = false;
    if (TrHs) then begin
      if (ato!=-1) then begin
        if (Excurr.SerNr>ato) then begin
          TrHs = false;
          skip = true;
        end;
      end; 
    end;
    if (RepSpec.flags[1]==0) then begin
      if blank(Excurr.VECode) then begin
        skip = false;
      end;
    end else begin
      if nonblank(Excurr.VECode) then begin
        skip = false;
      end;
    end;
    
    if (not skip) then begin
      tmpf = PrintDocument(Excurr,"ExcurDocForm",previewf);    
    end;
  end;
  EndPrintDialogGroup;
  RETURN;
END;
