global
updating function LongInt TimeClassVcRecordCheck(record TimeClassVc TimeClassr,record TimeClassVc TimeClass2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(TimeClassr.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1;
    goto LTimeClassVcRecordCheck;
  end;
LTimeClassVcRecordCheck:;
  TimeClassVcRecordCheck = res;
  RETURN;
END;
