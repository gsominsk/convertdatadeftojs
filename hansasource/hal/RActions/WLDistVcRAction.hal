global
function LongInt WLDistVcRecordCheck(var record WLDistVc WLDistr,record WLDistVc WLDist2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(WLDistr.DistCode)) then begin
    RecordCheckError(1058,"",-1,"DistCode");      
    res = -1; 
    goto LWLDistVcRecordCheck;
  end;
LWLDistVcRecordCheck:;
  WLDistVcRecordCheck = res;
  RETURN;
END;

