  AddEuroCurncyTrRow(gTRp,pmrw.AccNr,pmrw.Objects,"",pmrw.Comment,rs,false,dc2,totsum,
                     CLOutp.CurncyCode,frrate,torate1,torate2,baserate1,baserate2,CLOutp.Total-userval,nilv,-1,false,"");
*/  
