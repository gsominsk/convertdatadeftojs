global
updating function LongInt HRMCOClassVcRecordCheck(record HRMCOClassVc HRMCOCr,record HRMCOClassVc HRMCOC2r,LongInt stat,LongInt check)
begin
  LongInt res;

  res = 0;
  if (blank(HRMCOCr.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1;
    goto LHRMCOClassVcRecordCheck;
  end;
LHRMCOClassVcRecordCheck:;  
  HRMCOClassVcRecordCheck = res;
  return;
end;