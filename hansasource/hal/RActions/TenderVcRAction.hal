global
updating function LongInt TenderVcRecordCheck(record TenderVc Tenderr,record TenderVc Tender2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(Tenderr.Code)) then begin
    RecordCheckError(112,"",-1,"Code");      
    res = -1;
    goto LTenderVcRecordCheck;
  end;
LTenderVcRecordCheck:;
  TenderVcRecordCheck = res;
  RETURN;
END;
