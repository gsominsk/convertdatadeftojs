global
updating function LongInt HRMPACVcRecordCheck(record HRMPACVc HRMPACr,record HRMPACVc HRMR2r,LongInt stat,LongInt check)
begin
  LongInt res;

  res = 0;
  if (blank(HRMPACr.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1;
    goto LHRMPACVcRecordCheck;
  end;
LHRMPACVcRecordCheck:;  
  HRMPACVcRecordCheck = res;
  return;
end;