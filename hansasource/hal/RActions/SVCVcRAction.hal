global
updating function LongInt SVCVcRecordCheck(var record SVCVc SVCr,record SVCVc SVC2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;
  Integer insertmode;

  res = 0;     
  insertmode = 1;
  if (blank(SVCr.Code)) then begin
    RecordCheckError(1058,SVCr.Code,-1,"Code");      
    res = -1; 
    goto LSVCVcRecordCheck;
  end;
LSVCVcRecordCheck:;  
  SVCVcRecordCheck = res;  
  RETURN;
END;
