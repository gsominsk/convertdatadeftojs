global
function LongInt TelPersCodesVcRecordCheck(record TelPersCodesVc r1,record TelPersCodesVc r2,LongInt long3,LongInt long4)
begin
  boolean conds;
  LongInt res;

  res = 1;
  if blank(r1.Person) then begin
    RecordCheckError(1058,r1.Person,-1,"Person");
    goto LTelPersCodesVcRecordCheck;
  end;
  res = 0;
LTelPersCodesVcRecordCheck:;  
  TelPersCodesVcRecordCheck = res;
  return;
end;
