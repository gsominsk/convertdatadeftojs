
global
function LongInt HRMPymtTypeVcRecordInIndex(record HRMPymtTypeVc HRMPTr,string indexname)
begin
  LongInt res;
  
  res = 1;
  if (HRMPTr.Closed!=0) then begin 
    if (indexname=="ActCode") then begin res = 0; end;
    if (indexname=="ActHRMPymtTypeClass") then begin res = 0; end;
    if (indexname=="ActComment") then begin res = 0; end;
  end;
  HRMPymtTypeVcRecordInIndex = res;
  return;
end;
