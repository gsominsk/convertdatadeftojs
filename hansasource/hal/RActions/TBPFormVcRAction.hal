global
updating function LongInt TBPFormVcRecordCheck(record TBPFormVc TBPFormr,record TBPFormVc TBPForm2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(TBPFormr.Code)) then begin
    RecordCheckError(112,"",-1,"Code");      
    res = -1;
    goto LTBPFormVcRecordCheck;
  end;
LTBPFormVcRecordCheck:;
  TBPFormVcRecordCheck = res;
  RETURN;
END;
