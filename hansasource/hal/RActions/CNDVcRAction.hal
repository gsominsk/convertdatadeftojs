global
function LongInt CNDVcRecordCheck(record CNDVc CNDr,record CNDVc CND2p,LongInt stat,LongInt long4)
begin
  LongInt res;
      
  res = 0;
  if (blank(CNDr.Code)) then begin
    RecordCheckError(1120,"",-1,"Code");      
    res = -1; 
    goto LCNDVcRecordCheck;
  end;  
LCNDVcRecordCheck:;  
  CNDVcRecordCheck = res;
  return;
end;  
