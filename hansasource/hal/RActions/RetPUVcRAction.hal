  end;
  if (blank(RetPUp.OfficialSerNr)) then begin
    if (HasLocalization("PRT")) then begin
      if (RetPUp.Status!=kRecordStatusManual and RetPUp.Status!=kRecordStatusRecovered) then begin
        FindNextRetPUVcOfficialSerialNr(RetPUp,-1);
