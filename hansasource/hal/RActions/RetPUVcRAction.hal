  BlockLoad(LINrb);  
  if (nonblank(RetPUp.OfficialSerNr)) and ((MatRowCnt(LINrb)>0) or (HasLocalization("ARG,PRT"))) then begin
    GetLegalInvNrRow(RetPUp.OfficialSerNr,LINrbrw);
