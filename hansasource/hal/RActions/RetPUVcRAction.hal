  end;
  if ((stat==Rs_insert) or (RetPUp.SerNr!=RetPU2p.SerNr)) then begin
    locRetPUr.SerNr = RetPUp.SerNr;
    if (ReadFirstMain(locRetPUr,1,true)) then begin
      error = 1547;
      errorstr = "";
      rownr = -1;
      fieldstr = "SerNr";          
