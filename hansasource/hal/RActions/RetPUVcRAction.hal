    end;
  end;
  if (blanktime(RetPUp.PlanSendTime)) then begin
    if (HasLocalization("PRT")) then begin
      error = 20866;
      errorstr = "";
      rownr = -1;
      fieldstr = "PlanSendTime";          
      res = -1; 
      goto LRetPUVcRecordCheck;
    end;
  end;
  if (MSb.ForceReason!=0 and blank(RetPUp.Reason)) then begin
