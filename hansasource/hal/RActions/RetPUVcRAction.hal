  val qty;

  qty = 0;
  rwcnt = MatRowCnt(RetPUr);
  for (i=0;i<rwcnt;i=i+1) begin
    MatRowGet(RetPUr,i,RetPUrw);
    if (RetPUrw.ArtCode==ArtCode) and (RetPUrw.SerialNr==SerialNr) then begin
      qty = qty + RetPUrw.Quant;
    end;
  end;
  SerialNrOnThisRetPU = qty;
  RETURN;
