    if (check>0) and (true) then begin
    if (nonblank(RetPUrw.ArtCode)) then begin
      if (ReadFirstItem(RetPUrw.ArtCode,INr,true,true)==false) then begin
        error = 1120;
        errorstr = RetPUrw.ArtCode;
