        switch (INr.SerNrf) begin
          case 1:  // serial nr. 
            if (SNrb.BulkSerialNos!=0) then begin
              errcode = ValidateBulkSerialNoRow(SNrb,RetPUp,RetPUrw,i,errstr,j);
