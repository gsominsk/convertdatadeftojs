
global
function LongInt OBTypeVcRecordCheck(var record OBTypeVc OBTyper,record OBTypeVc OBType2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;
  
  res = 0;
  if (blank(OBTyper.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1; 
    goto LOBTypeVcRecordCheck;
  end;
LOBTypeVcRecordCheck:;
  OBTypeVcRecordCheck = res;
  RETURN;
END;
