global
function LongInt ScheduleRuleVcRecordCheck(record ScheduleRuleVc Scheduler,record ScheduleRuleVc Schedule2p,LongInt stat,LongInt long4)
begin
  LongInt res;
      
  res = 0;
  if (blank(Scheduler.Code)) then begin
    RecordCheckError(1120,"",-1,"Code");      
    res = -1; 
    goto LScheduleRuleVcRecordCheck;
  end;  
LScheduleRuleVcRecordCheck:;  
  ScheduleRuleVcRecordCheck = res;
  return;
end;  
