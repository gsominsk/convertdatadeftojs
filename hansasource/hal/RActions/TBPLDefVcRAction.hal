global
updating function LongInt TBPLDefVcRecordCheck(record TBPLDefVc TBPLDefr,record TBPLDefVc TBPLDef2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(TBPLDefr.Code)) then begin
    RecordCheckError(112,"",-1,"Code");      
    res = -1;
    goto LTBPLDefVcRecordCheck;
  end;
LTBPLDefVcRecordCheck:;
  TBPLDefVcRecordCheck = res;
  RETURN;
END;
