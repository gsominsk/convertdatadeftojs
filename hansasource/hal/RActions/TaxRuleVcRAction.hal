global
function LongInt TaxRulesVcRecordInIndex(record TaxRulesVc TaxRulesr,string indexname)
begin
  LongInt res;
  
  res = 1;
  if (TaxRulesr.VATType!=kVATTypeGrossIncomePerception) then begin
    if (indexname=="GIPCode")  then begin res = 0; end;
    if (indexname=="GIPComment")  then begin res = 0; end;
    if (indexname=="GIPTaxCode")  then begin res = 0; end;
  end;
  TaxRulesVcRecordInIndex = res;
  return;
end;