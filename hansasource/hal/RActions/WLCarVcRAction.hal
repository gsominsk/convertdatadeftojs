global
function LongInt WLCarVcRecordCheck(var record WLCarVc WLCarr,record WLCarVc WLCar2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;

  res = 0;
  if (blank(WLCarr.CarCode)) then begin
    RecordCheckError(1058,"",-1,"CarCode");      
    res = -1; 
    goto LWLCarVcRecordCheck;
  end;
LWLCarVcRecordCheck:;
  WLCarVcRecordCheck = res;
  RETURN;
END;

