global
updating function LongInt HRMSkillVcRecordCheck(record HRMSkillVc HRMSr,record HRMSkillVc HRMS2r,LongInt stat,LongInt check)
begin
  LongInt res;

  res = 0;
  if (blank(HRMSr.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1;
    goto LHRMSkillVcRecordCheck;
  end;
LHRMSkillVcRecordCheck:;  
  HRMSkillVcRecordCheck = res;
  return;
end;