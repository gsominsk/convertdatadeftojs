
global
function LongInt IntVcRecordCheck(var record IntVc Intr,record IntVc Int2r,LongInt stat,LongInt long4)
BEGIN
  LongInt res;
  
  res = 0;
  if (blank(Intr.Code)) then begin
    RecordCheckError(1058,"",-1,"Code");      
    res = -1; 
    goto LIntVcRecordCheck;
  end;
LIntVcRecordCheck:;
  IntVcRecordCheck = res;
  RETURN;
END;
