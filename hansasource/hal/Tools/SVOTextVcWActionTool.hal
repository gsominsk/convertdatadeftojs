external function Boolean GetSVOSerialItem(string,string,record SVOSerVc);

global
function Boolean SVOTextVc_PasteSerialNr(var record SVOTextVc SVOTextp)
BEGIN
  Boolean res;
  record SVOSerVc SVOSerr;
  
  if (GetSVOSerialItem(SVOTextp.ArtCode,SVOTextp.SerialNr,SVOSerr)) then begin
    SVOTextp.ArtCode = SVOSerr.ItemCode;
    res = true;
  end else begin
    res = false;
  end;
  SVOTextVc_PasteSerialNr = res;
  RETURN;
END;
