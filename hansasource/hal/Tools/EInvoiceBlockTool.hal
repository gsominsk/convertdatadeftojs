external updating procedure InitIdleTasks_EInvoiceOnOKWindow(record EInvoiceBlock,record EInvoiceBlock);

global
updating procedure EInvoiceTClassOnOKWindowRemote(record EInvoiceBlock EIBb,record EInvoiceBlock EIBprevb)
begin

  if (EIBb.StopEInvoices!=0) then begin 
    goto LEInvoiceTClassOnOKWindowRemote;
  end;
  InitIdleTasks_EInvoiceOnOKWindow(EIBb,EIBprevb);
LEInvoiceTClassOnOKWindowRemote:;
  return;
end;