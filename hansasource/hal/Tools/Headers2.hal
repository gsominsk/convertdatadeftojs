global
procedure HTAccs(string a,string b,Integer uo, var string res)
begin
  string 255 tstr;

  if (blank(a) and blank(b)) then begin
   if (uo==1) then begin
    res = USetStr(3004);
   end else begin
    res = USetStr(3005);
   end;
  end else begin
   if (uo==1) then begin
    tstr = USetStr(3006);
   end else begin
    tstr = USetStr(3007);
   end;
   if (a==b) then begin
    res = USetStr(3008);
    res = res & a;
   end else begin
    res = tstr & a & " : " & b;
   end;
  end;
return;
end;

global
procedure HTVends(string a, string b, var string res)
begin
  record VEVc VEr;

  if (blank(a) and blank(b)) then begin
    res = USetStr(1212);
  end else begin
    res = USetStr(1213);
    res = res & a;
    if (a <> b) then begin
      res = res & " : " & b;
    end else begin
      VEr.Code = a;
      if ReadFirstMain(VEr,1,true) then begin
        res = res & "  " & VEr.Name;
      end;
    end;
  end;
  return;
end;

