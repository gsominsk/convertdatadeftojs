global
procedure HTSalesmanName(string usercode,var string res)
begin
  record UserVc Userr;
  
  res = "";
  if (nonblank(usercode)) then begin
    Userr.Code = usercode;
    if (FindTheUser(Userr)) then begin
      res = USetStr(8314);
      res = res & " " & usercode;
      res = res & " : " & Userr.Name;
    end;  
  end;
  return;
end;
