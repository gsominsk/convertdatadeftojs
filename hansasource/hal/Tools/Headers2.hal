global
procedure HTPrintedf(Integer a,Integer b, var string res)
begin
  res = "";
  if ((a==0) and (b==0)) then begin
   res = USetStr(1148);
  end;
  if ((a!=0) and (b==0)) then begin
   res = USetStr(1146);
  end;
  if ((a==0) and (b!=0)) then begin
   res = USetStr(1147);
  end;
return;
end;

global
procedure HTOKF(Integer a,Integer b, var string tstr)
begin
  tstr = "";
  if (a==0 and b==0) then begin
   tstr = USetStr(1136);
  end;
  if (a<>0 and b==0) then begin
   tstr = USetStr(1137);
  end;
