external function Boolean GetItemNameStr(Integer,var string,string,string,string);

global
function Boolean ReplItemsVc_PasteSerialNr(var record ReplItemsVc ReplItemsp)
BEGIN
  Boolean res;
  record INVc INr;
  string 255 tstr;
  
  INr.Code = ReplItemsp.ArtCode;
  if (ReadFirstMain(INr,1,true)) then begin
    if (GetItemNameStr(1,tstr,"",INr.Name,INr.Code)) then begin end;  
    ReplItemsp.Desc = tstr;
    res = true;
  end;
  ReplItemsVc_PasteSerialNr = res;
  RETURN;
END;
