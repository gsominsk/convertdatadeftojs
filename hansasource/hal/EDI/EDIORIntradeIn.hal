/* WORKS */
    if (lstr=="UNH") then begin
      nrofrows = 0;
      ORrw.ArtCode = "";
      ORrw.SalesAcc = "";
      ORrw.Objects = "";
      ORrw.Spec = "";
      ORrw.VATCode = "";
      ORrw.Recepy = "";
      ORrw.SerialNr = "";
      ORrw.VARList = "";
      ORrw.Location = "";
      ORrw.Source = "";
      ORrw.PickingTime = "";
      ORrw.InloadingTime = "";
      ORrw.DespatchRowTime = "";
