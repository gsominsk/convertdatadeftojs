        CUr.Code = mstr;
        if (ReadFirstMain(CUr,1,true)) then begin
          mstr = MID(tstr,31,35);
          StripEndingSpaces(mstr);
          ORr.Addr0 = mstr;
          mstr = MID(tstr,66,35);
          StripEndingSpaces(mstr);
          ORr.Addr1 = mstr;
          mstr = MID(tstr,136,35);
          StripEndingSpaces(mstr);
          ORr.Addr2 = mstr;
          PasteEDICust(CUr,ORr);
        end;
      end;
      if (pstr == "SE") then begin
      end;
      if (pstr == "CN") then begin   
      //if (pstr == "DP") then begin  
        mstr = MID(tstr,31,35);
        StripEndingSpaces(mstr);
        if (nonblank(mstr)) then begin
