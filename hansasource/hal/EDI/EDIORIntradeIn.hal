        end;  
// We can't handle more than 255 character long strings
// So I will skip the last line and use that line from the Customer
//        mstr = MID(tstr,276,35);
//        StripEndingSpaces(mstr);
//        ORr.ShipAddr2 = mstr;
//        mstr =  MID(tstr,241,9);
//        StripEndingSpaces(mstr);
//        ORr.ShipAddr2 = ORr.ShipAddr2 & " " & mstr;
      end;
