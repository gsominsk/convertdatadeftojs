          ORrw.Price = price;
          ORrw.vRebate = reb;
          ORrw.BasePrice = cost;
          ORrw.Spec = curitemname;
          ORrw.VATCode = vatcode;
          ORrw.SalesAcc = salesacc;
        end; 
