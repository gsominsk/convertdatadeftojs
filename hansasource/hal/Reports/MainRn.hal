global
procedure OutputReportIndent(record RCVc RepSpec,Integer cnt)
begin
  Integer i;
  
  if (RepSpec.Media==mtFile or RepSpec.Media==mtExcel) then begin
    for (i=0;i<cnt;i=i+1) begin
      OutString(0,0,"",false);
    end;
  end;
