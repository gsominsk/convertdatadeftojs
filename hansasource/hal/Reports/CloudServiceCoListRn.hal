external procedure HTCusts(string, string, var string);globalprocedure CloudServiceCoListRn(record RcVc RepSpec)begin  record TXCUServiceVc TXCUSr;  Boolean found,testf;  string 255 frcu,tocu,tstr;  Integer rw;  frcu = FirstInRange(RepSpec.f1,20);  tocu = LastInRange(RepSpec.f1,20);  StartReportJob(USetStr(34330));  HTCusts(frcu,tocu,tstr);  Header(rw,tstr,1);  rw = rw + 1;  EndHeader;  SetRepCol(2,50);  SetRepCol(3,120);  SetRepCol(4,230);  SetRepCol(5,280);  SetRepCol(6,340);  SetRepCol(7,400);  SetRepCol(8,440);  StartFormat(15);   OutString(0,0,USetStr(34331),false);   OutString(2,0,USetStr(34332),false);   OutString(3,0,USetStr(34333),false);   OutString(4,0,USetStr(34334),false);   OutString(5,0,USetStr(34335),false);   OutString(8,0,USetStr(34336),false);  EndFormat;  Gray_Divider(0,1);  found = true;  while (LoopMain(TXCUSr,1,found)) begin    if (found) then begin      testf = true;      if (nonblank(RepSpec.f1)) then begin        if (TXCUSr.CustCode<frcu) then begin testf = false; end;        if (TXCUSr.CustCode>tocu) then begin testf = false; end;      end;      if (nonblank(RepSpec.f2)) then begin        if (TXCUSr.Service!=RepSpec.f2) then begin testf = false; end;      end;              if (RepSpec.flags[1]!=0) then begin        if (TXCUSr.NoInvoicing==0) then begin testf = false; end;      end;      if (RepSpec.flags[2]!=0) then begin        if (TXCUSr.Closed==0) then begin testf = false; end;      end;      if (TXCUSr.Type!=RepSpec.flags[3]) then begin testf = false; end;      if (nonblankdate(TXCUSr.StartDate)) then begin        if (nonblankdate(RepSpec.d1)) then begin          if (TXCUSr.StartDate<RepSpec.d1) then begin testf = false; end;        end;      end;      if (nonblankdate(TXCUSr.EndDate)) then begin        if (nonblankdate(RepSpec.d2)) then begin          if (TXCUSr.EndDate>RepSpec.d2) then begin testf = false; end;        end;      end;      if (testf) then begin        StartFormat(15);         OutLongInt(0,"DblTXCUServiceVc",TXCUSr.SerNr,false);         OutString(2,"DblCUVc",TXCUSr.CustCode,false);         OutString(3,0,TXCUSr.Service,false);         OutDate(4,0,TXCUSr.StartDate,false);         OutDate(5,0,TXCUSr.EndDate,false);         if (TXCUSr.NoInvoicing!=0) then begin           OutString(6,0,USetStr(34337),false);         end;         if (TXCUSr.Closed!=0) then begin           OutString(7,0,USetStr(34338),false);         end;         switch (TXCUSr.Type) begin           case 0: tstr = USetStr(34339);           case 1: tstr = USetStr(34340);           case 2: tstr = USetStr(34341);         end;         OutString(8,0,tstr,false);        EndFormat;        StartFormat(15);         OutString(2,0,TXCUSr.CustName,false);        EndFormat;      end;    end;  end;  EndJob;  return;end;