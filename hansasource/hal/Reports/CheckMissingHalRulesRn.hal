external procedure GetMissingFileList(array string);globalprocedure CheckMissingHalRulesRn(record RCVc RepSpec)begin  array string 255 aList;  integer i;    StartReportJob(UsetStr(33333));    EndHeader;    GetMissingFileList(aList);  for (i=0;i<aList.length;i=i+1) begin    StartFormat(15);    OutString(0,0,aList[i],false);    EndFormat;  end;  EndJob;  return;end;