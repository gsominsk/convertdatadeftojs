BEGIN
  val res,v;
  record IVVc IVr;
  row IVVc IVrw;
  Integer i,rwcnt;
  Boolean found,testf;
  string 30 ckey;
  Integer keys;
  record IVCashVc IVCashr;
  row IVCashVc IVCashrw;
  record RestAccVc RAr;
  row RestAccVc RArw;
  record TaxMatrixVc TMr;
  record INVc INr;
  record SMVc VATr;
