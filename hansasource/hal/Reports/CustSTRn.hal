                  v1 = 0;
                end;
                v = MulRateToBase1(RArw.CurncyCode,RArw.Sum+v1,RArw.FrRate,RArw.ToRateB1,RArw.ToRateB2,RArw.BaseRate1,RArw.BaseRate2,DefaultCurRoundOff);
            end;
            res = res + v;                                        
          end;
        end;
      end;
    end;
  end;
  IVGetTurnover2 = res;
  RETURN;
END;

