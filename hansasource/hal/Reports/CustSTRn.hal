      tstr = USetStr(8318);
      tstr = tstr & ": ";
      tstr = tstr & RepSpec.Stext;        
      Header(rw,tstr,0);
    end;
    HTCusts(frcc,tocc,tstr);
    Header(rw,tstr,1);
    rw = rw + 1;
    if (nonblank(RepSpec.f2)) then begin
      HTArts(RepSpec.f2,RepSpec.f2,tstr);
      Header(rw,tstr,1);
      rw = rw + 1;
    end;
    HTCustCat(RepSpec.f3,tstr);
    if (nonblank(tstr)) then begin
      Header(rw,tstr,1);
      rw = rw + 1;
    end;  
    if (ProgramType<>4) then begin
      HTSaleMan(RepSpec.AccStr,tstr);
      if (nonblank(tstr)) then begin
        Header(rw,tstr,1);
        rw = rw + 1;
      end;  
    end;  
    tal = RepSpec.vals0;
    if (nonblank(tal)) then begin
      tstr = USetStr(2498);
      tstr = tstr & ": ";
      tstr = tstr & tal;
      Header(rw,tstr,1);
      rw = rw + 1;
    end;
    tal = RepSpec.vals1;
    if (nonblank(tal)) then begin
      tstr = USetStr(2499);
      tstr = tstr & ": ";
      tstr = tstr & tal;
