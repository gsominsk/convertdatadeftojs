          if (RArw.stp!=kInvoiceRowTypeNormal and RArw.stp!=kInvoiceRowTypeDownpayment and RArw.stp!=kInvoiceRowTypeCorrection and RArw.stp!=kInvoiceRowTypeCorrection) then begin
