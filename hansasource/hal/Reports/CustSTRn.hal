            testf = true;
            if (IVrw.stp!=kInvoiceRowTypeNormal and IVrw.stp!=kInvoiceRowTypeDownpayment and IVrw.stp!=kInvoiceRowTypeCorrection and IVrw.stp!=kInvoiceRowTypeCorrection) then begin
