procedure DumpTextFieldToReport(record RcVc RepSpec)begin  LongInt l,lcnt;  string 255 tstr;  lcnt = LineTextCnt(RepSpec);  for (l = 0; l<lcnt; l = l + 1) begin    tstr = LineTextGet(RepSpec,l);    StartFormat(15);      OutString(0,0,tstr,false);    EndFormat;  end;  return;end;globalprocedure ASPScriptRn(record RcVc RepSpec)begin  StartReportJob(RepSpec.f2);  EndHeader;  DumpTextFieldToReport(RepSpec);  EndJob;  return;end;