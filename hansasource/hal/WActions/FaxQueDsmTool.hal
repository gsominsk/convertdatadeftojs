global
updating procedure Server_ClearFaxQueLsm()
BEGIN
  record FaxQueVc FaxQuer;
  boolean TrHs;
  
  TrHs = true;
  FaxQuer.SegNr = -1;
  ResetLoop(FaxQuer);
  while (LoopMain(FaxQuer,1,TrHs)) begin
    if (TrHs) then begin
      RecordDelete(FaxQuer);
      StepBack(FaxQuer);
    end;
  end;
  RETURN;
END;
