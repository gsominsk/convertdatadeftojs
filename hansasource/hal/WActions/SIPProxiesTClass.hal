global
procedure SIPProxiesTClassQueuesbtn()
begin
  record RcVc RepSpec;
  string 255 subset;
  LongInt l;
  Integer wn;

  wn = CurWindow;
  l = SelectedListLine(wn);
  if (l>-1) then begin
    subset = GetListString(wn,l);
  end;
  OpenWindow("SIPQueuesTClass",1,CurWindow,subset,"",RepSpec);
  return;
end;

global
procedure BCSipProxiesBtn()
begin
  record RcVc RepSpec;
  OpenWindow("SIPProxiesTClass",1,CurWindow,"","",RepSpec);
  return;
end;
