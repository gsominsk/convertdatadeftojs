remote procedure RsrtEventScheduleVc_PasteEventType(var record RsrtEventVc);

function Boolean RsrtEventScheduleDClassEventTypeEFAfter(Integer wn,Boolean changedf)
begin
  Boolean res;
  record RsrtEventScheduleVc RESr;
  
  if (changedf) then begin
    GetWindowRecord(wn,RESr);
    RsrtEventScheduleVc_PasteEventType(RESr);
    PutWindowRecord(wn,RESr);
  end;
  RsrtEventScheduleDClassEventTypeEFAfter = res;
  return;
end;

global
function Boolean RsrtEventScheduleDClassAfterEditField(Integer wn,string fieldname,Integer fn, Integer rownr,Integer changed)
begin
  Boolean res;

  switch (fieldname) begin
    case "EventType": res = RsrtEventScheduleDClassEventTypeEFAfter(wn,changed!=0);
  end;
  RsrtEventScheduleDClassAfterEditField = res;
  return;
end;

