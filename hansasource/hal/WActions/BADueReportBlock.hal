remote procedure BADueReportTClassOnOKWindowRemote(record BADueReportBlock,record BADueReportBlock);

global
function Boolean BADueReportTClassOnOKWindow(Integer wn)
begin
  record BADueReportBlock BADRb;
  record BADueReportBlock BADRprevb;

  GetWindowRecord(wn,BADRb);  
  BlockLoad(BADRprevb);
  BADueReportTClassOnOKWindowRemote(BADRb,BADRprevb);
  BADueReportTClassOnOKWindow = true;
  return;
end;