global
procedure PrintCOCQStatD(Integer wn,Boolean previewf)
BEGIN
  record COCQStatVc COCQStatr;
  record RcVc RepSpec;
  
  GetWindowRecord(wn,COCQStatr);
  RepSpec.repname = "CQStatRn";
  if (previewf) then begin
    RepSpec.Media = mtScreen;
  end else begin
    RepSpec.Media = mtPrinter;
  end;
  RepSpec.JobDf = 0;
  RepSpec.f1 = COCQStatr.COSerNr;
  RunReport(RepSpec,0);
  RETURN;
END;