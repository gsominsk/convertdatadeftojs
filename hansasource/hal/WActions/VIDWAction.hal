procedure VIDClassSpecPasteNameObjects(Integer wn,var string psname)
BEGIN
  record VIVc VIr;
  row VIVc VIrw;
  Integer rownr;
  string 200 typ;
  string 10 accnr;

  GetWindowRecord(wn,VIr);
  rownr = WindowActiveRow(wn);
