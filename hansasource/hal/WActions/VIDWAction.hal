function Boolean VIDClassVECodeEFAfter(Integer wn,Integer changedf)
begin
  Boolean res;
  record VIVc VIr;
  record VIVc oldVIr;
  row VIVc VIrw;
  Integer rwcnt;
  string 255 vewarn;

  if (changedf!=0) then begin  
    res = true;
    GetWindowRecord(wn,VIr);
    RecordCopy(oldVIr,VIr);
    if (VIVc_PasteVECode(VIr,WindowState(wn),true,true,vewarn)==false) then begin
      res = false;
    end;
    if (res) then begin
      if (nonblank(vewarn)) then begin    
        MessageBox(0,vewarn);
      end;
      PutWindowRecord(wn,VIr);
      if (oldVIr.ExportFlag!=VIr.ExportFlag) then begin
        rwcnt = MatRowCnt(VIr) - 1;
        if (rwcnt>=0) then begin
          GetWindowRecord(wn,VIr);
          VIDDefault(rwcnt,"Sum",VIr);
          MatRowGet(VIr,rwcnt,VIrw);
          if (VIrw.Sum==0) then begin VIrw.Sum = blankval; end;
          MatRowPut(VIr,rwcnt,VIrw);
          PutWindowRecord(wn,VIr);
          VIDClassSumEFAfter(wn,rwcnt,changedf);
        end;
      end;
    end;
  end;
  VIDClassVECodeEFAfter = res;
  return;
end;
 
