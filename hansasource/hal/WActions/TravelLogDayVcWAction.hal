remote procedure TravelLogDayVc_CalcSum(var record TravelLogDayVc);

function Boolean TravelLogDClassCountryCodeEFAfter(Integer wn,Integer changed)
begin
  record TravelLogDayVc TravelLogDayr;
  
  if (changed!=0) then begin
    GetWindowRecord(wn,TravelLogDayr);
    TravelLogDayVc_CalcSum(TravelLogDayr);
    PutWindowRecord(wn,TravelLogDayr);
  end;
  
  return;
end;

global
function Boolean TravelLogDayDClassAfterEditField(Integer wn,string fieldname,Integer fn, Integer rownr,Integer changed)
begin
  Boolean res;

  switch (fieldname) begin
    case "CountryCode": res = TravelLogDClassCountryCodeEFAfter(wn,changed);
  end;
  
  TravelLogDayDClassAfterEditField = res;
  return;
end;

function Boolean TravelLogDClassDeductButtonAfter(Integer wn)
begin
  record TravelLogDayVc TravelLogDayr;
  
  DeselectWindow(wn,false);
  GetWindowRecord(wn,TravelLogDayr);
  TravelLogDayVc_CalcSum(TravelLogDayr);
  PutWindowRecord(wn,TravelLogDayr);
  
  return;
end;

