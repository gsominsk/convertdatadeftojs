
global
function Boolean AmexStatementIClassOnOKWindow(Integer wn)
begin
  boolean res;
  record RcVc RepSpec;
  
  res = true;
  if (wn>0) then begin 
    GetWindowRecord(wn,RepSpec);
    if (blank(RepSpec.f1)) then begin 
      MessageBox(1058,"");
      WindowFieldGoTo(wn,RepSpec,-1,"f1",true);
      res = false;
    end;
  end;

  AmexStatementIClassOnOKWindow = res;
  return;
end;


