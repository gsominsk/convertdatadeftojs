global
procedure AT2HistDsm()
BEGIN
  record AT2UnitVc ATUnitr;
  record RcVc RepSpec;
  Integer wn;

  wn = CurWindow;
  GetWindowRecord(wn,ATUnitr);
  RepSpec.repname = "AT2HistRn";
  RepSpec.f1 = ATUnitr.InventoryNr;
  RepSpec.Media = mtScreen;
  RepSpec.sStartDate = ATUnitr.PurchaseDate;
  RepSpec.sEndDate = CurrentDate;
  RepSpec.flags[1] = 1;
  RepSpec.flags[2] = 1;
  RepSpec.flags[3] = 1;
  RepSpec.flags[4] = 1;
  RunReport(RepSpec,0);
  RETURN;
END;