external function string 40 SerialNrSClassSpecPName(string);

global
function string 40 InspectConDClassSpecPasteName(Integer wn,string defpsname)
begin
  string 255 psname;
  
  psname = defpsname;
  switch (WindowActiveField(wn)) begin
    case "SerialNr": psname = SerialNrSClassSpecPName(defpsname);
  end;
  InspectConDClassSpecPasteName = psname;
  return;
end;
