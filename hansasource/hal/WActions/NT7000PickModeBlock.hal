external function Boolean ForkLiftSystemStatus(string);

global
function Boolean NT7000PickModeTClassActiveEditField(Integer wn,string fieldname,Integer fn,Integer wnst,Integer rownr,Integer changed)
BEGIN
  Boolean res;
  record LocalMachineBlock LMb;

  BlockLoad(LMb);
  res = ForkLiftSystemStatus(LMb.DefLocation);
  if (res==false) then begin
    MessageBox(1728,"");
  end;
  NT7000PickModeTClassActiveEditField = res;
  RETURN;
END;

